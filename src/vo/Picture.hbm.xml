<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.Picture" table="picture" schema="bbs">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="picture">
            <column name="picture" sql-type="char(30)" length="30"/>
        </property>
        <property name="uploadTime">
            <column name="upload_time" sql-type="datetime"/>
        </property>
        <set name="followpostPictures" inverse="true" lazy="false">
            <key>
                <column name="picture_id"/>
            </key>
            <one-to-many not-found="ignore" class="vo.FollowpostPicture"/>
        </set>
        <set name="postPictures" inverse="true" lazy="false">
            <key>
                <column name="picture_id"/>
            </key>
            <one-to-many not-found="ignore" class="vo.PostPicture"/>
        </set>
        <set name="users" inverse="true" lazy="false">
            <key>
                <column name="picture_id"/>
            </key>
            <one-to-many not-found="ignore" class="vo.User"/>
        </set>
    </class>
</hibernate-mapping>