<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.PostPicture" table="post_picture" schema="bbs">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <many-to-one name="post" class="vo.Post" lazy="false">
            <column name="post_id"/>
        </many-to-one>
        <many-to-one name="picture" class="vo.Picture" lazy="false">
            <column name="picture_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>