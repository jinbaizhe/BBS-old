<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.Collection" table="collection" schema="bbs">
        <id name="id" column="id">
            <generator class="increment"></generator>
        </id>
        <property name="time" column="time"/>
        <many-to-one name="user" class="vo.User" lazy="false">
            <column name="user_id"/>
        </many-to-one>
        <many-to-one name="post" class="vo.Post" lazy="false">
            <column name="post_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>