<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.FollowpostPicture" table="followpost_picture" schema="bbs">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"></generator>
        </id>
        <many-to-one name="followpost" class="vo.Followpost" lazy="false">
            <column name="followpost_id"/>
        </many-to-one>
        <many-to-one name="picture" class="vo.Picture" lazy="false">
            <column name="picture_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>